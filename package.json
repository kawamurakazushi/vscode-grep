{
  "name": "vscode-grep",
  "displayName": "vscode-grep",
  "description": "",
  "version": "0.1.0",
  "publisher": "kawamurakazushi",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:grep.git"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "grep.git",
        "title": "Grep: Git"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "@types/shell-quote": "^1.6.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "shell-quote": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kawamurakazushi/vscode-grep"
  }
}
